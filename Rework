                        (itemText.includes('Si lees esto, ignorame') && sectionId === 'saludo')) {
                        item.classList.add('active');
                    }
                });
            }
            window.closeSidebar();
        }

        window.copyScript = function(button, scriptId) {
            const scriptElement = document.getElementById(scriptId);
            if (!scriptElement) return;
            const scriptText = scriptElement.textContent;
            if (navigator.clipboard && navigator.clipboard.writeText) {
                navigator.clipboard.writeText(scriptText).then(() => showCopySuccess(button)).catch(() => fallbackCopy(scriptText, button));
            } else {
                fallbackCopy(scriptText, button);
            }
        }

        function showCopySuccess(button) {
            if (!button) return;
            const originalText = button.textContent;
            button.textContent = '‚úÖ ¬°Copiado!';
            button.classList.add('copied');
            setTimeout(() => {
                button.textContent = originalText;
                button.classList.remove('copied');
            }, 2000);
        }

        function fallbackCopy(text, button) {
            try {
                const textArea = document.createElement('textarea');
                textArea.value = text;
                textArea.style.position = 'fixed';
                textArea.style.left = '-999999px';
                textArea.style.top = '-999999px';
                document.body.appendChild(textArea);
                textArea.focus();
                textArea.select();
                const successful = document.execCommand('copy');
                document.body.removeChild(textArea);
                if (successful) showCopySuccess(button);
            } catch (err) {}
        }

        window.redirectToURL = function(button) {
            window.location.href = "https://pastebin.com/raw/Ru4UQDpN";
        }

        document.addEventListener('DOMContentLoaded', () => {
            createParticles();
            window.showContent('welcome');
        });

        document.addEventListener('mouseover', e => {
            const card = e.target.closest('.script-card');
            if (card) card.style.transform = 'translateY(-8px) scale(1.02)';
        });

        document.addEventListener('mouseout', e => {
            const card = e.target.closest('.script-card');
            if (card) card.style.transform = 'translateY(0) scale(1)';
        });
    </script>
</body>
</html>
