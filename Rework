        window.showContent = function(sectionId) {
            const welcomeScreen = document.getElementById('welcome');
            if (welcomeScreen) {
                welcomeScreen.style.display = 'none';
            }
            const contents = document.querySelectorAll('.content');
            contents.forEach(content => content.classList.remove('active'));
            const sidebarItems = document.querySelectorAll('.sidebar-item');
            sidebarItems.forEach(item => item.classList.remove('active'));
            if (sectionId === 'welcome') {
                if (welcomeScreen) welcomeScreen.style.display = 'flex';
                if (sidebarItems[0]) sidebarItems[0].classList.add('active');
            } else {
                const targetContent = document.getElementById(sectionId);
                if (targetContent) targetContent.classList.add('active');
                sidebarItems.forEach(item => {
                    const itemText = item.textContent.trim();
                    if ((itemText.includes('99 NOCHES') && sectionId === '99-noches-en-el-bosque') ||
                        (itemText.includes('ARISE') && sectionId === 'arise-crossover') ||
                        (itemText.includes('ATTACK') && sectionId === 'attack-on-titan-revolution') ||
                        (itemText.includes('BASICALLY FNF') && sectionId === 'basically-fnf-remix') ||
                        (itemText.includes('BLOCK SPIN') && sectionId === 'block-spin') ||
                        (itemText.includes('BLOODMONEY') && sectionId === 'bloodmoney') ||
                        (itemText.includes('BLOX FRUITS') && sectionId === 'blox-fruits') ||
                        (itemText.includes('BLUE LOCK') && sectionId === 'blue-lock-rivals') ||
                        (itemText.includes('DANDY') && sectionId === 'dandy-world') ||
                        (itemText.includes('DEAD DEFENSE') && sectionId === 'dead-defense') ||
                        (itemText.includes('DEAD RAILS') && sectionId === 'dead-rails') ||
                        (itemText.includes('DEMON BLADE') && sectionId === 'demon-blade') ||
                        (itemText.includes('DEMON FALL') && sectionId === 'demon-fall') ||
